{
  "manifest_version": 3,
  "name": "GeoGuessr AZERTY Fix",
  "version": "1.0",
  "description": "Active les raccourcis d'emotes (1-6) pour les claviers AZERTY fran√ßais sur GeoGuessr",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.geoguessr.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}